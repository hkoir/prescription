<style>
    

.hero-section {
    position: relative;
    background: linear-gradient(to bottom, rgba(0, 0, 0, 0.5), rgba(255, 255, 255, 1)),
                url('https://source.unsplash.com/1600x900/?office,teamwork') no-repeat center center/cover;
    height: 70vh;
    display: flex;
    justify-content: center;
    align-items: center;
    color: white;
    text-align: center;
    overflow: hidden;
    padding: 0 15px;
    border-bottom: 1px solid #eee; /* Optional: A light border for a clean separation */
}


    .hero-overlay {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.6); /* Darker and more professional */
        z-index: 1;
    }

    .hero-content {
        position: relative;
        z-index: 2;
        max-width: 800px;
        margin: 0 0;
        text-shadow: 0px 2px 4px rgba(0, 0, 0, 0.6); /* Subtle shadow for contrast */
        animation: fadeInUp 10s ease-out infinite;
    }

    .hero-section h1 {
        font-size: 3.5rem;
        font-weight: 700;
        letter-spacing: 1px;
        margin-bottom: 0.5rem;
    }

    .hero-section p {
        font-size: 1.3rem;
        font-weight: 300;
        opacity: 0.95;
        line-height: 1.6;
        margin-bottom: 1.5rem;
    }

    /* Optional Animation */
    @keyframes fadeInUp {
        from {
            opacity: 0;
            transform: translateY(40px);
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    /* Optional Button */
    .hero-btn {
        background-color: #fcb900;
        color: #333;
        padding: 12px 30px;
        font-size: 1rem;
        font-weight: 600;
        text-transform: uppercase;
        border: none;
        border-radius: 8px;
        transition: all 0.3s ease;
        cursor: pointer;
        box-shadow: 0px 4px 12px rgba(0, 0, 0, 0.2);
    }

    .hero-btn:hover {
        background-color: #f5a623;
        color: #fff;
        transform: translateY(-3px);
    }



.custom-shape-divider-bottom-hero {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    overflow: hidden;
    line-height: 0;
    }
.custom-shape-divider-bottom-hero svg {
        position: relative;
        display: block;
        width: calc(115% + 1.3px);
        height: 80px;
    }
.custom-shape-divider-bottom-hero .shape-fill {
        fill: #ffffff;
    }






ul.custom-arrow-list li::before {
    content: "\21D2";  
    color: blue;  
    font-size: 1rem;  
    margin-right: 10px; 
    font-weight: bold; 
}

ul.custom-arrow-list li {
    margin-bottom: 5px; 
    font-family: Arial, sans-serif; 
    font-size: 1rem; 
    color: #333333; 
}



.module-icon {
        font-size: 40px;
        color: #007bff;
    }
.feature-section {
        padding: 60px 0;
        background-color: white;
    }

    
.cta-section {
        background-color: #007bff;
        color: white;
        padding: 40px 0;
        text-align: center;
    }



.module-card {
    transition: transform 0.3s ease, box-shadow 0.3s ease; /* Smooth transitions */
    overflow: hidden; /* Needed for inner shadow effect */
    left: 0;
    width:100%;
}

.module-card:hover {
    transform: translateY(-5px); /* Move card up slightly */
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2); /* More pronounced shadow */
    left: 0;
    width:100%
}

.module-card::before { /* Inner shadow on hover */
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0); /* Transparent initially */
    transition: background-color 0.3s ease;
    pointer-events: none; /* Allows clicks to pass through */
}


.module-card:hover::before {
  background-color: rgba(0, 0, 0, 0.1); /* Semi-transparent black on hover */
}
.module-card .card-body {
    transition: transform 0.3s ease; /* Smooth transitions for content */
}

.module-card:hover .card-body {
    transform: scale(1.02); /* Slightly scale up the content */
}



/* Optional: Change button color on hover */
.module-card:hover .btn-outline-primary {
    background-color: #007bff;
    color: white;
    border-color: #007bff;
}


.feature-box,.price_card {
    padding: 20px; /* Add some padding around the content */
    border: 1px solid #eee; /* Light border */
    border-radius: 8px; /* Rounded corners */
    transition: transform 0.3s ease, box-shadow 0.3s ease; /* Smooth transitions */
    
    color: #333 !important; /* Set a visible color */
  
   
}


.feature-box:hover,.price_card:hover{
    transform: translateY(-5px); /* Move box up slightly */
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.15); /* Subtler shadow */
    border-color: #ddd; /* Slightly darker border on hover */
    background-color: rgba(0, 0, 0, 0.1); /* Semi-transparent black on hover */
    color: #333 !important; /* Set a visible color */
   
}



.feature-box h4 {
    margin-bottom: 10px;
}




@keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
       }
   
@keyframes shake {
    0%, 100% { transform: translateX(0); }
    25% { transform: translateX(-5px); }
    50% { transform: translateX(5px); }
    75% { transform: translateX(-5px); }
   }


.live_demo_box {
    padding: 20px; 
    border: 1px solid #eee; 
    border-radius: 8px; 
    transition: transform 0.3s ease, box-shadow 0.3s ease; 
    cursor: pointer;
    animation: shake 5s ease-in-out infinite;
}



.live_demo_box:hover {
    transform: translateY(-5px); 
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.15); 
    border-color: #ddd; 
    background-color: rgba(0, 0, 0, 0.1);
}




.feature-box2{
    padding: 20px; 
    border: 1px solid #eee; 
    border-radius: 8px; 
    transition: transform 0.3s ease, box-shadow 0.3s ease; 
    
    color: #333 !important; 
  
   
}


.feature-box2:hover{
